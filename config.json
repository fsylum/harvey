{
  "autoprefixer": [
    "last 2 versions",
    "> 1%",
    "ie >= 9",
    "android >= 4.4"
  ],
  "clean": ["static"],
  "css": {
    "src": "src/scss/**/*.scss",
    "dest": "static/css"
  },
  "img": {
    "src": "src/img/*.{gif,jpg,png}",
    "dest": "static/img"
  },
  "js": {
    "src": "src/js/**/*.js",
    "dest": "static/js"
  }
}
